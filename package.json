{
  "name": "styleguide-components",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@researchgate/react-intersection-observer": "^1.0.5",
    "@styled-system/css": "^5.1.4",
    "@styled-system/theme-get": "^5.1.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^8.0.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@xstate/react": "^0.8.1",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "from": "^0.1.7",
    "import": "^0.0.6",
    "intersection-observer": "^0.7.0",
    "lodash": "^4.17.15",
    "polished": "^3.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "styled": "^1.0.0",
    "styled-components": "^4.4.1",
    "styled-system": "^5.1.4",
    "typescript": "~3.7.2",
    "xstate": "^4.7.6"
    "typescript": "~3.7.4"
  },
  "prettier": "@will-stone/prettier-config",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "eslint --fix --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "prettier/prettier": "error",
      "simple-import-sort/sort": "off",
      "import/no-unresolved": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.0",
    "@storybook/addon-links": "^5.3.0",
    "@storybook/addons": "^5.3.0",
    "@storybook/preset-create-react-app": "^1.5.0",
    "@storybook/react": "^5.3.0",
    "@types/lodash": "^4.14.149",
    "@types/styled-components": "^4.4.2",
    "@types/styled-system": "^5.1.4",
    "@will-stone/prettier-config": "^2.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "prettier": "^1.19.1"
  }
}
